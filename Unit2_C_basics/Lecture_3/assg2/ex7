#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int n ,i;
	unsigned long int fact =1 ;

	printf("Enter an Integer:");
	fflush(stdout);
	scanf("%d",&n) ;
	if(n<0)
	{printf("Error !!! Factorial of negative number doesn't exist.");
	fflush(stdout); }
	if (n>0)
	{	for (i=1;i<=n;i++)
		fact=fact*i ;
	printf("Factorial = %lu",fact);
	fflush(stdout); }
	return 0 ;
}
