
#include <stdio.h>
int main(void) {
	float a ,b,c ;
	printf("enter 2 values a than b respectively:");
	fflush(stdout);
	scanf("%f %f",&a,&b);
	fflush(stdin);
	c=a;
	a=b;
	b=c;
	printf("After swapping, value of a = %.2f\n",a);
	fflush(stdout);
	printf("After swapping, value of b = %.2f",b);
	fflush(stdout);
	return 0 ;
}
